<Window x:Class="MaterMinds.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MaterMinds"
        mc:Ignorable="d"
        Title="Matermind" Height="600" Width="800">

    <Grid>
        <StackPanel>
            <Button x:Name="BtnOne" Content="New Game" HorizontalAlignment="Left" Margin="350,150,0,0" VerticalAlignment="Top" Click="BtnOne_Click" Width="71"/>
            <Button x:Name="BtnTwo" Content="Select Player" HorizontalAlignment="Left" Margin="350,10,0,0" VerticalAlignment="Top" Click="BtnTwo_Click"/>
            <Button x:Name="BtnThree" Content="Highscore" HorizontalAlignment="Left" Margin="350,10,0,0" VerticalAlignment="Top" Click="BtnThree_Click" Width="71"/>
        </StackPanel>
            <Frame x:Name="Main" NavigationUIVisibility="Hidden">
            </Frame>
    </Grid>
</Window>
